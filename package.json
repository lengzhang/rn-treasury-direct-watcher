    "devDependencies": {
        "@babel/core": "^7.20.0",
        "@types/ramda": "^0.29.12",
        "@types/react": "~18.2.45",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-config-universe": "^12.0.1",
        "eslint-plugin-prettier": "^5.1.3",
        "prettier": "^3.2.5",
        "typescript": "^5.1.3"
