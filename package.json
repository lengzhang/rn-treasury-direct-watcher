{
    "name": "rn-treasury-direct-watcher",
    "version": "1.0.0",
    "main": "src/App.tsx",
    "scripts": {
        "start": "expo start",
        "dev": "expo start --dev-client",
        "android": "expo start --android",
        "ios": "expo start --ios",
        "web": "expo start --web",
        "ts:check": "tsc",
        "lint": "eslint .",
        "lint:fix": "eslint --fix .",
        "prettier:check": "prettier . --check",
        "prettier:write": "prettier . --write",
        "build:ios:dev": "eas build --platform ios --profile development",
        "build:ios:preview": "eas build --platform ios --profile preview",
        "build:ios:prod": "eas build --platform ios --profile production",
        "submit:ios:prod": "eas submit --platform ios --profile production",
        "update:ios:prod": "eas update --channel production",
        "update:ios:preview": "eas update --channel preview",
        "expo:upgrade": "yarn add expo@latest && expo install --fix"
    },
    "dependencies": {
        "@gluestack-style/react": "^1.0.52",
        "@gluestack-ui/config": "^1.1.18",
        "@gluestack-ui/themed": "^1.1.22",
